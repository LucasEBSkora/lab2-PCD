<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" fillWidth="false"
      xmlns="http://javafx.com/javafx/17.0.2-ea" fx:controller="eu.telecomnancy.lab2e2455u.view.EntryScreenHeader">
    <children>
        <HBox>
            <children>
                <Button fx:id="yesterdayButton" mnemonicParsing="false" onAction="#backOneDay" text="Button"/>
                <Label fx:id="todayLabel" style="-fx-font-size: 30;" text="Label"/>
                <Button fx:id="tomorrowButton" mnemonicParsing="false" onAction="#forwardOneDay" text="Button"/>
            </children>
        </HBox>
        <TextField fx:id="titleField" alignment="CENTER" promptText="Titre"/>
    </children>
</VBox>
